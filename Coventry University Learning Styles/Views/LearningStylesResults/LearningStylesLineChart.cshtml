@model IEnumerable<decimal>

@{
    string myTheme = @" <Chart BackColor=""#eeeeee"">
                            <ChartAreas>
                                <ChartArea Name=""Default"" BackColor=""Transparent""></ChartArea>
                            </ChartAreas>
                        </Chart> ";
}

@{
    Chart LearningStylesLineChart = new Chart(width: 500, height: 400, theme: myTheme)
    .AddTitle("Learning Styles Line Chart")
    .AddSeries(
        name: "Learning Style Line Chart",
        chartType: "Line",
        xValue: new[] { "Aural %", "Kineasthetic %", "Read/Write %", "Visual %" },
        yValues: Model)
    .Write();
}